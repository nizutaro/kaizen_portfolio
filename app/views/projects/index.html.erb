<h2 class="page-header text-center border-bottom">提案一覧</h2>
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <%= link_to "提案する",new_project_path, {class: "btn btn-success"} %>
  </div>
</div>
    <table class="table table-striped table-inverse">
      <th>No.</th>
      <th>タイトル</th>
      <th>提案者</th>
      <% @projects.each do |project| %>
        <tr>
          <td><%= project.id %></td>
          <td><%= project.name %></td>
          <td><%=  project.user.family_name + project.user.farst_name %></td>
        </tr>
     <% end %>
    </table>