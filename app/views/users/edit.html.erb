<h2 class="page-header text-center">登録情報編集</h2><br>
  <div class="row center">
    <div class="col-md-8 col-md-offset-2">
      <div class= "form-group">
        <%= form_with(model:[@user], local: true) do |f| %>
      </div>
      <table>
        <tr>
          <td>
            <div class= "form-group">
                <%= f.label :"(姓)" %>
                <%= f.text_field :family_name, class: 'form-control', placeholder: "例:田中" %>
            </div>
            </td>
            <td>　</td>
          <td>
            <div class= "form-group">
                <%= f.label :"(名)" %>
              <%= f.text_field :farst_name, class: 'form-control', placeholder: "例:太郎" %>
            </div>
          </td>
        </tr>
      </table>
                
      <%= f.attachment_field :image %><br>
      <div class= 'text-center'>
        <%= f.submit '編集する', class:"btn btn-primary" %>
      </div>
      <% end %>
    </div>
  </div>